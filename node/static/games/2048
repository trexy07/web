<!--<script type="text/python">

#shapes/drawing
from base import *

# Example usage:
drawRect(0, 0, 400, 400, "white")
drawRect(50, 50, 100, 60, "blue",'red',5)
# drawRect(0, 0, 200, 200, "blue")
drawOval(200, 50, 100, 60, "red")
drawCircle(100, 150, 40, "green")
drawLine(20, 20, 200, 200, "purple")
drawLabel("Hello, World!", 50, 300, "black")
drawRegularPolygon(300, 150, 6, 40, "orange")
drawStar(150, 300, 5, 40, 20, "pink")
drawPolygon([(250, 250), (300, 300), (350, 250)], "brown")
drawArc(250, 300, 100, 60, 0, 180, "blue", "lightblue")

## web getting
# data=fetch_data('https://trevinspi.freeddns.org/games/api.php?mode=1&num=0')
# exec("data="+data)


data=get_uno(0)

print(data)

drawLabel(data, 50, 50, "black")

global_down=False
def mouseMove(ev):
    global global_down
    if global_down:
        mouseDrag
    else:
        drawLabel(f"{ev.x},{ev.y}",ev.x,ev.y,fill=rgb(255,200,100))

def mouseDown(ev):
    global global_down
    global_down=True
    drawLabel(f"{ev.x},{ev.y}",ev.x,ev.y,fill=rgb(0,200,100))

def mouseUp(ev):
    global global_down
    global_down=False
    drawLabel(f"{ev.x},{ev.y}",ev.x,ev.y,fill=rgb(199, 5, 61))


document["myCanvas"].bind("mousemove", mouseMove)
document["myCanvas"].bind("mousedown", mouseDown)
document["myCanvas"].bind("mouseup", mouseUp)


import time
last=time.time()
def onStep():
    global last
    
    new=time.time()
    print(new-last)
    last=new
    
    #required option a or b, dynamic step interval
    window.setTimeout(onStep, 1000)

# option b, set step iterval
# window.setInterval(onStep, 1000)    
    
onStep()





</script> -->

